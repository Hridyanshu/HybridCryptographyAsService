<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>HybridCryptography</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
        <servlet-name>main</servlet-name>
        <jsp-file>/main.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/main</url-pattern>
  </servlet-mapping>
  
  <servlet>
        <servlet-name>download-encrypted</servlet-name>
        <jsp-file>/downloadEncrypted.html</jsp-file>
  </servlet>
  <servlet-mapping>
        <servlet-name>download-encrypted</servlet-name>
        <url-pattern>/download-encrypted</url-pattern>
  </servlet-mapping>
  
  <servlet>
        <servlet-name>download-decrypted</servlet-name>
        <jsp-file>/downloadDecrypted.html</jsp-file>
  </servlet>
  <servlet-mapping>
        <servlet-name>download-decrypted</servlet-name>
        <url-pattern>/download-decrypted</url-pattern>
  </servlet-mapping>
  
  <servlet>
        <servlet-name>download</servlet-name>
        <jsp-file>/fileDownload.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/download</url-pattern>
  </servlet-mapping> 

  <servlet>
        <servlet-name>uploadCloud</servlet-name>
        <jsp-file>/cloudUpload.html</jsp-file>
  </servlet>
  <servlet-mapping>
        <servlet-name>uploadCloud</servlet-name>
        <url-pattern>/uploadCloud</url-pattern>
  </servlet-mapping>
  
  <servlet>
        <servlet-name>upload</servlet-name>
        <jsp-file>/cloudUpload.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
  </servlet-mapping>
</web-app>
